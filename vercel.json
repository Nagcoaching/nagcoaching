{
  "redirects": [
    { "source": "/individuel", "destination": "/coaching-solo.html", "permanent": true },
    { "source": "/duo", "destination": "/coaching-duo.html", "permanent": true },
    { "source": "/coaching-en-duo", "destination": "/coaching-duo.html", "permanent": true },
    { "source": "/groupe", "destination": "/coaching-groupe.html", "permanent": true },
    { "source": "/small-group", "destination": "/coaching-groupe.html", "permanent": true },
    { "source": "/distance", "destination": "/coaching-distance.html", "permanent": true },
    { "source": "/entreprise", "destination": "/coaching-entreprise.html", "permanent": true },
    { "source": "/teambuilding", "destination": "/team-building.html", "permanent": true },
    { "source": "/team-building-sportif", "destination": "/team-building.html", "permanent": true },
    { "source": "/studio/vert-saint-denis", "destination": "/studio.html", "permanent": true },
    { "source": "/index.html", "destination": "/", "permanent": true },
    { "source": "/index", "destination": "/", "permanent": true }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "X-XSS-Protection", "value": "1; mode=block" }
      ]
    },
    {
      "source": "/src/images/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/src/css/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/src/js/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
